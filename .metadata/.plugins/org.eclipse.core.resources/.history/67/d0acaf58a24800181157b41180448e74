{

	public static void main(String[] args) 
{
		WebDriver driver=new FirefoxDriver();
		driver.get("http://facebook.com");
		Select drop=new Select(driver.findElement(By.xpath("//*[@id='month']")));
		drop.selectByIndex(0);
		
		

		
	}

}
