package com.MyBasics;

public class Test 
{
	public static void main(String[] args)
	{
	Test obj=new Test();
	obj.add(10, 20);
	obj.sum(10, 20, 30);

	}


public void add(int a,int b)
{
	int c=a+b;
	System.out.println(c);
	//return 0;
}
public void sum(int a,int b,int c)
{
	int d=a+b+c;
	System.out.println(d);
	//return 0;
	
}
}




